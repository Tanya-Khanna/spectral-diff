{
  "enabled": true,
  "name": "Regen Demo Snapshot",
  "description": "Rebuild when demo data changes",
  "version": "1",
  "when": {
    "type": "fileEdited",
    "patterns": [
      "apps/web/src/lib/demo/**"
    ]
  },
  "then": {
    "type": "runCommand",
    "command": "pnpm -C apps/web build"
  }
}